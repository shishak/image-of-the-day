	<meta property="og:type" content="{{ item.description }}">
	<meta property="og:url" content="{{ item.path }}">
    <title>{{ item.title }}</title>
    <style>{% include "css/styles.css" %}</style>
  </head>
  <body>
    <header>
      <div class="container">
        Medium Feed Export / <a href="/">articles</a> / <a href="/get-yours">get yours</a>
      </div>
